<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/appColorPrimary"
    android:orientation="vertical">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightBlue"
        android:onClick="toCheckZjc"
        android:text="CheckZjcActivity"
        android:textColor="@color/appColorPrimary" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightBlue"
        android:onClick="toLeadInAccount"
        android:text="LeadInAccountActivity"
        android:textColor="@color/appColorPrimary" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightBlue"
        android:onClick="toWalletZjc"
        android:text="WalletZjcActivity"
        android:textColor="@color/appColorPrimary" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightBlue"
        android:onClick="toAccountManage"
        android:text="AccountManageActivity"
        android:textColor="@color/appColorPrimary" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightBlue"
        android:onClick="toPasswordCheck"
        android:text="PasswordCheckActivity"
        android:textColor="@color/appColorPrimary" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightBlue"
        android:onClick="toMain"
        android:text="Main"
        android:textColor="@color/appColorPrimary" />

</LinearLayout>
