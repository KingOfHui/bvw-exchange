    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/appColorPrimary</item>
        <item name="colorPrimaryDark">@color/appColorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!-- 设置activity切换动画 -->
        <!--<item name="android:windowAnimationStyle">@style/activityAnimation</item>-->
        <!-- Customize color of navigation drawer icon and back arrow -->
        <item name="colorBackgroundFloating">@color/white</item>
    </style>
    <style name="AppTheme.Appbar">
        <item name="actionMenuTextColor">@color/menu_text_color</item>
        <item name="overlapAnchor">false</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="colorControlNormal">@color/white</item>
    </style>
    <style name="AppTheme.FullScreen">
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>
    <style name="Line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/line</item>
    </style>
    <style name="OptionItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <declare-styleable name="BubbleImageView">
        <attr format="dimension|reference" name="bubble_arrowTop"/>
        <attr format="dimension|reference" name="bubble_arrowWidth"/>
        <attr format="dimension|reference" name="bubble_arrowHeight"/>
        <attr format="dimension|reference" name="bubble_arrowOffset"/>
        <attr format="dimension|reference" name="bubble_angle"/>
        <attr format="enum" name="bubble_arrowLocation">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr format="boolean" name="bubble_showText"/>
        <attr format="boolean" name="bubble_showShadow"/>
        <attr format="boolean" name="bubble_showArrow"/>
    </declare-styleable>
    <declare-styleable name="LoadingView">
        <attr format="color" name="loadding_color"/>
        <attr format="enum" name="ring_style">
            <enum name="square" value="0"/>
            <enum name="round" value="1"/>
        </attr>
        <attr format="enum" name="progress_style">
            <enum name="material" value="0"/>
            <enum name="linear" value="1"/>
        </attr>

        <attr format="dimension" name="ring_width"/>
        <attr format="dimension" name="ring_radius"/>
    </declare-styleable>
    <declare-styleable name="OptionItemView">
        <attr format="string" name="title"/>
        <attr format="dimension" name="title_size"/>
        <attr format="color" name="title_color"/>
        <attr format="integer" name="badge_count"/>
        <attr format="reference|color" name="start_src"/>
        <attr format="reference|color" name="end_src"/>
        <attr format="string" name="desc"/>
        <attr format="dimension" name="desc_size"/>
        <attr format="color" name="desc_color"/>
        <attr format="boolean" name="show_arrow_indicator"/>
        <attr format="boolean" name="divider_align_to_title"/>
    </declare-styleable>
</resources>